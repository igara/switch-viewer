{
  "name": "switch-viewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts",
    "watch": "webpack --watch",
    "start": "electron .",
    "package:mac": "electron-packager . switch-viewer --platform=darwin --arch=x64 --electronVersion=8.2.0 --overwrite",
    "package:win": "electron-packager . switch-viewer --platform=win32 --arch=x64 --electronVersion=8.2.0 --overwrite",
    "clean:all": "rm -rf node_modules switch-viewer-darwin-x64 dist"
  },
  "main": "dist/index.js",
  "dependencies": {
    "base64url": "3.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "css-loader": "5.0.1",
    "electron": "11.1.1",
    "electron-connect": "0.6.3",
    "electron-packager": "15.2.0",
    "electron-rebuild": "2.3.4",
    "electron-reload-webpack-plugin": "2.0.4",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "file-loader": "6.2.0",
    "node-loader": "1.0.2",
    "prettier": "2.2.1",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.14",
    "typed-css-modules-webpack-plugin": "0.2.0",
    "typescript": "4.1.3",
    "webpack": "5.13.0",
    "webpack-cli": "4.3.1",
    "webpack-env": "0.8.0"
  }
}
